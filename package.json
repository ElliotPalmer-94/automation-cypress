    "cy:run:firefox": "cypress run --browser firefox --record --headless",
    "cy:run:edge": "cypress run --browser edge --record --headless",
    "prelighthouse": "mkdir -p ./cypress/reports/lighthouse",
    "lighthouse": "lighthouse http://the-internet.herokuapp.com --output=html --output-path=./cypress/reports/lighthouse/example.html"
